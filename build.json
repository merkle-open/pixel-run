{
    "clean": {
        "dist": "./dist/**/*.*"
    },
    "bundle": {
        "name": "bundle",
        "target": "./dist"
    },
    "dependencies": {
        "target": "./dist",
        "name": "dependencies",
        "files": [
            "./bower_components/jquery/dist/jquery.min.js",
            "./bower_components/phaser/build/phaser.js",
            "./bower_components/browser-storage/browser-storage.min.js",
            "./app/provider.js",
            "./app/settings.js"
        ]
    },
    "app": {
        "minify": false,
        "target": "./dist",
        "name": "application",
        "files": [
            "./app/util/*.js",
            "./app/classes/*.js",
            "./app/states/*.js",
            "./app/main.js"
        ]
    }
}
