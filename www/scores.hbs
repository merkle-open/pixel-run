<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Pixel. Run. Namics. - {{title}}</title>
    <link rel="stylesheet" href="dist/bin/app.min.css">
    <link rel="icon" href="dist/assets/img/favicon.png" type="image/png">
</head>
<body>
    <div class="bgimage gradient-green gradient-animated">
        <div class="content">
            <div class="grid">
                <div class="col-1-1">
                    <div class="score-wrapper paper-layer">
                        <table>
                            <thead>
                                <tr>
                                    <td>Platz</td>
                                    <td>Score</td>
                                    <td>Name</td>
                                    <td>Welt</td>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each scores}}
                                    <tr data-pid="{{this.$id}}">
                                        <td>
                                            <strong>#{{index}}</strong>
                                        </td>
                                        <td>{{this.score}}</td>
                                        <td>{{this.name}}</td>
                                        <td>{{this.world}}</td>
                                    </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="dist/dependencies.js"></script>
    <script src="dist/application.js"></script>
</body>
</html>
