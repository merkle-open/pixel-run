{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "globals": {
    "Phaser": true
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["__express"] }],
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "specialLink": ["Link"]
      }
    ]
  }
}
